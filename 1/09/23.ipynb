        "def lonelynumber(n):\n",
        "    s=[]\n",
        "    for i in n:\n",
        "        v=n.count(i)\n",
        "        if v<2:\n",
        "            s.append(i)\n",
        "    print(s)\n",
        "lonelynumber([2,1,4,2,1,5,4,7])"
